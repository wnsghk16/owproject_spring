<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.legacy.myapp.mappers.OWPlalyerMapper">
    <select id="selectAll" resultType="com.legacy.myapp.domains.OWPlayerDTO">
        select
            id,
            player,
            playerimg,
            name,
            hometown,
            teamimg,
            team,
            role,
        from owplayer
    </select>
</mapper>